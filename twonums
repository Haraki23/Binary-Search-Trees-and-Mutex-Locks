//Code to find the sum of two numbers in an array / Brute Force O(n2)
//Author: Seifullah Elharaki
class Solution {
    public int[] twoSum(int[] nums, int target) {
        int i;
        int j;
        int [] twonums = {0,0};
        for(i=0;i<nums.length;i++){

        for(j=0;j<nums.length;j++){
            //i is not equal to j so we dont return the same index let's say we have a target of 6 and we have [3,5] we dont want the answer to be [0,0]
            if ((nums[i] + nums[j] == target) && (i!=j)){
                twonums[0]=i;
                twonums[1]=j;
            return twonums;                
            }            
        }
        }
            return twonums;
    }
}
